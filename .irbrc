IRB.conf[:SAVE_HISTORY] = 100000
IRB.conf[:USE_AUTOCOMPLETE] = false

autoload :CGI, 'cgi'
autoload :CSV, 'csv'
autoload :Date, 'date'
autoload :DateTime, 'date'
autoload :FileUtils, 'fileutils'
autoload :JSON, 'json'
autoload :OpenStruct, 'ostruct'
autoload :Pathname, 'pathname'
autoload :SecureRandom, 'securerandom'
autoload :Set, 'set'
autoload :YAML, 'yaml'
