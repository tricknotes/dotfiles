[user]
    name  = Ryunosuke SATO
    email = tricknotes.rs@gmail.com
[alias]
    a = add -p
    s = status
    c = commit -v
    ck = checkout
    df = diff
    bc = branch
    lo = log --pretty=oneline --graph
    rclean = "!for branch in `git branch -r`; do git branch -rd $branch 2>/dev/null; done"
[color]
    ui = auto
[github]
    user = rsato
[core]
    excludesfile = ~/global.gitignore
